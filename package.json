{
	"name": "fitnessapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "pnpm run smui-theme-light && pnpm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme ./node_modules",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark ./node_modules"
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@smui/button": "7.0.0-beta.8",
		"@smui/card": "7.0.0-beta.8",
		"@smui/dialog": "7.0.0-beta.8",
		"@smui/icon-button": "7.0.0-beta.8",
		"@smui/tab": "7.0.0-beta.8",
		"@smui/tab-bar": "7.0.0-beta.8",
		"@smui/textfield": "7.0.0-beta.8",
		"@smui/top-app-bar": "7.0.0-beta.8",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.16.3",
		"@types/locomotive-scroll": "^4.1.0",
		"@types/nodemailer": "^6.4.8",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.28.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"smui-theme": "7.0.0-beta.8",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.7",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.4.0",
		"chart.js": "^4.3.0",
		"chartjs-adapter-date-fns": "^3.0.0",
		"locomotive-scroll": "^4.1.4",
		"nodemailer": "^6.9.2",
		"pnpm": "^8.5.1",
		"postmark": "^3.0.18",
		"svelte-email": "^0.0.4"
	}
}
